<%= form_with model: [:app, @user], url: app_project_membership_account_setup_path(@membership.signed_id(purpose: :project_invitation)) do |form| %>
  <%= form.label :full_name %>
  <%= form.text_field :full_name %>

  <%= form.label :password %>
  <%= form.password_field :password %>

  <%= form.submit "Save changes" %>
<% end %>